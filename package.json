{
  "name": "model-builder",
  "version": "1.0.5",
  "description": "Reduce boiler plate on testing",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/microtema/model-builder-ts"
  },
  "keywords": [
    "Model",
    "Builder",
    "Test",
    "Unit"
  ],
  "author": "microtema <7fate@web.de>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/microtema/model-builder-ts/issues"
  },
  "homepage": "https://github.com/microtema/model-builder-ts#readme",
  "devDependencies": {
    "@types/jest": "^22.2.3",
    "jest": "^23.0.1",
    "prettier": "^1.13.2",
    "ts-jest": "^22.4.6",
    "tslint": "^5.10.0",
    "tslint-config-prettier": "^1.13.0",
    "typescript": "^2.8.3"
  }
}
